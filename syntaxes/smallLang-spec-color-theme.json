{
	"name": "smallLang-spec-theme",
	"type": "dark",
	"include": "./dark_plus.json",
	"colors": {
		"editor.foreground": "#D4D4D4"
	},
	"tokenColors": [
		{
			"name": "proc / pred / aux / pre / post / enum / type / wp",
			"scope": [
				"keyword.control.slspec"
			],
			"settings":{
				"foreground": "#F6386D",
				"fontStyle": "italic"
			}
		},
		{
			"name": "if  / fi / while / endwhile / skip / :=",
			"scope": [
				"keyword.function.slspec"
			],
			"settings":{
				"fontStyle": "italic",
				"foreground": "#569cd6"
			}
		},
		{
			"name": "∑ | sum / ∏ | prod",
			"scope": [
				"support.function.slspec"
			],
			"settings":{
				"fontStyle": "italic bold",
				"foreground": "#569cd6"
			}
		},
		{
			"name": "then  / else / do",
			"scope": [
				"keyword.function.inner.slspec"
			],
			"settings":{
				"fontStyle": "",
				"foreground": "#82d8ee"
			}
		},
		{
			"name": "from / to",
			"scope": [
				"support.function.inner.slspec"
			],
			"settings":{
				"fontStyle": "",
				"foreground": "#82d8ee"
			}
		},
		{
			"name": "in / out / inout",
			"scope": [
				"variable.parameter.slspec"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#89ebb2"
			}
		},
		{
			"name": "ℤ / ℝ / Char / Bool / T / ⨉ / seq[]",
			"scope": [
				"entity.name.type.slspec",
				"entity.name.type.seq.slspec"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#4EC9B0"
			}
		},
		{
			"name": "true / false / ⊥",
			"scope": [
				"constant.language.slspec"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#2a9d8f"
			}
		},
		{
			"name": "números",
			"scope": [
				"constant.numeric.slspec"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#B5CEA8"
			}
		},
		{
			"name": "carácteres",
			"scope": [
				"constant.character.slspec"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#CE9178"
			}
		},
		{
			"name": "operadores lógicos",
			"scope": [
				"keyword.operator.logic.slspec"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#F6386D"
			}
		},
		{
			"name": "operadores matemáticos",
			"scope": [
				"keyword.operator.math.slspec"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#ec96af"
			}
		},
		{
			"name": "parentesis",
			"scope": [
				"punctuation.parenthesis.slspec"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#ec96af"
			}
		},
		{
			"name": "comentario multilínea",
			"scope": [
				"comment.block.slspec"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#546E7A"
			}
		},
		{
			"name": "comentario de línea",
			"scope": [
				"comment.line.slspec"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#546E7A"
			}
		}
	]
}
